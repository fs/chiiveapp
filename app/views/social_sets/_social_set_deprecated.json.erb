<%
concat "{\"#{social_set.uuid}\":{"
concat "\"owner_uuid\":\"#{social_set.default_user.uuid}\","
concat "\"title\":\"#{social_set.title}\","
concat "\"time_at\":#{social_set.time_at.to_json},"
concat "\"posts_count\":#{social_set.posts.size},"
# concat "\"personal_sets_count\":#{social_set.personal_sets.size},"
if (defined?(posts_limit) and posts_limit == 1 && social_set.posts.size > 0)
	concat "\"post\":["
	concat render :partial => "posts/post_deprecated", :locals => { :post => social_set.posts.first }
	concat "],"
else
	concat "\"posts\":["
	concat social_set.posts.map { |post| render :partial =>  "posts/post_deprecated", :locals => { :post => post } }.join(",")
	concat "],"
end
concat "\"users\":["
concat social_set.users.map { |user|  render :partial => "users/user_deprecated", :locals => { :user => user } }.join(",")
concat "]"
concat "}}"
%>